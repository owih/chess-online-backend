{
  "version": 2,
  "buildCommand": "npx prisma generate && npm run build",
  "builds": [
    {
      "src": "npx prisma generate && dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}
